{
  "config": {
    "submit": "https://prod-00.canadacentral.logic.azure.com:443/workflows/0d67d751882a4eadbfbdff33d5c3a3eb/triggers/manual/paths/invoke?api-version=2016-06-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=Fta8i2jva5cfgU6wnokmmPcKhkCjmGp4JIR9DXEXLpg",
    "siteKey": "4fbfd303f2f74c6ea2ec3bc9ab44d454",
    "successUrl": "",
    "theme": "",
    "externalId": ""
  },
  "head": {
    "title": "Project Retrospective Form",
    "logo": "https://uploads-ssl.webflow.com/5e0e0d786de93874eb27e0c1/5e138d756b2f0315a9133ee5_AFTIA%20Logo%20Color.svg"
  },
  "body": {
    "items": [
      {
        "type": "text",
        "label": "Company Name",
        "placeholder": "",
        "bind": "company",
        "width": "full"
      },
      {
        "type": "text",
        "label": "Your name",
        "placeholder": "",
        "required": true,
        "bind": "name",
        "width": "full"
      },
      {
        "type": "text",
        "label": "Your Role",
        "placeholder": "",
        "required": true,
        "bind": "role",
        "width": "full"
      },
      {
        "type": "textarea",
        "label": "What problem were you aiming to solve or what results were you trying to achieve by working with AFTIA?",
        "required": true,
        "bind": "problem",
        "width": "full"
      },
      {
        "type": "dropdown",
        "label": "How satisfied were you with AFTIAâ€™s services?",
        "width": "full",
        "required": true,
        "bind": "satisfactionlvl",
        "options": [
          {
            "label": "Very Unsatisfied",
            "value": "Very Unsatisfied"
          },
          {
            "label": "Somewhat Unsatisfied",
            "value": "Somewhat Unsatisfied"
          },
          {
            "label": "Neither Satsified nor Unsatisfied",
            "value": "3Neither Satsified nor Unsatisfied"
          },
          {
            "label": "Somewhat Satsified",
            "value": "Somewhat Satsified"
          },
          {
            "label": "Very Satsified",
            "value": "Very Satsified"
          }
        ]
      },
      {
        "type": "textarea",
        "width": "full",
        "bind": "moreaboutsatisfaction",
        "label": "Please tell us more about the reason for your satisfactiion level",
        "required": false
      },
      {
        "type": "textarea",
        "label": "What challenges does the implemented solution alleviate?",
        "placeholder": "",
        "required": true,
        "bind": "challenges",
        "width": "full"
      },
      {
        "type": "textarea",
        "label": "What went well with the project and were there any challenges or obstacles?",
        "placeholder": "",
        "required": true,
        "bind": "wentwell",
        "width": "full"
      },
      {
        "type": "radio",
        "label": "Would you recommend AFTIA's services to other companies?",
        "bind": "recommend",
        "options": [
          {
            "label": "Yes",
            "value": "Yes"
          },
          {
            "label": "No",
            "value": "No"
          },
          {
            "label": "Maybe",
            "value": "Maybe"
          }
        ]
      },
      {
        "type": "text",
        "label": "If yes then who?",
        "bind": "yesthenwho",
        "required": false
      },
      {
        "type": "textarea",
        "width": "full",
        "label": "Is there anyone at AFTIA that you would like to give kudos to?",
        "bind": "kudos",
        "required": false
      },
      {
        "type": "radio",
        "required": true,
        "width": "full",
        "bind": "continuework",
        "label": "Do you plan to continue working with us or are interested in working together again in the future?",
        "options": [
          {
            "label": "Yes",
            "value": "Yes"
          },
          {
            "label": "No",
            "value": "No"
          },
          {
            "label": "Maybe",
            "value": "Maybe"
          }
        ]
      },
      {
        "type": "radio",
        "label": "I grant my permission to feature my testimonial on the AFTIA website.",
        "bind": "permissiontest",
        "options": [
          {
            "label": "Yes",
            "value": "Yes"
          },
          {
            "label": "No",
            "value": "No"
          }
        ]
      },
      {
        "type": "textarea",
        "label": "Please provide a 2-3 public facing testimonial on your experience working with AFTIA.",
        "width": "full",
        "required": false,
        "bind": "expwithAftia"
      },
      {
        "type": "radio",
        "width": "full",
        "label": "I grant my permission for my company logo from our website, or the image uploaded below, to be used on the AFTIA website.",
        "bind": "permissionlogo",
        "options": [
          {
            "label": "Yes",
            "value": "Yes"
          },
          {
            "label": "No",
            "value": "No"
          }
        ]
      },
      {
        "type": "file",
        "label": "Please upload an image of your company logo, yourself or your team.",
        "required": false,
        "width": "full",
        "bind": "fileattacment"
      },
      {
        "type": "textarea",
        "label": "Please provide any final comments you would like to share.",
        "required": false,
        "bind": "finalcomments",
        "width": "full"
      },
            {
        "type": "radio",
        "bind": "managertalk",
        "required": true,
        "width": "full",
        "label": "Would you like to be contacted by an AFTIA manager to discuss your experience further?",
        "options": [
          {
            "label": "Yes",
            "value": "Yes"
          },
          {
            "label": "No",
            "value": "No"
          }
        ]
      },
      {
        "type": "radio",
        "bind": "learnmore",
        "required": true,
        "width": "full",
        "label": "This form was made with Morf, did you find it easy to use and want to learn more about this product? ",
        "options": [
          {
            "label": "Yes",
            "value": "Yes"
          },
          {
            "label": "No",
            "value": "No"
          }
        ]
      },
      {
        "type": "submit",
        "label": "Submit"
      }
    ]
  }
}
